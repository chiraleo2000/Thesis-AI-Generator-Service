# Google Cloud Configuration
GCP_PROJECT_ID=your_project_id
GCP_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/service_account.json

# Gemini Configuration
GOOGLE_API_KEY=your_gemini_api_key_here
GEMINI_MODEL_VERSION=gemini-1.5-pro-preview-0409
MAX_OUTPUT_TOKENS=8192

# Cloud Document AI (OCR) configuration
DOCUMENT_AI_LOCATION=us
DOCUMENT_AI_PROCESSOR_ID=your_processor_id_here

# Google Search Configuration (Programmable Search Engine)
GOOGLE_SEARCH_API_KEY=your_search_api_key_here
GOOGLE_SEARCH_CX=your_search_engine_id_here

# Cloud SQL Configuration
# Format: project:region:instance
CLOUD_SQL_CONNECTION_NAME=project:region:instance
DB_USER=postgres
DB_PASS=password
DB_NAME=thesis_ai_db

# App Configuration
GRADIO_SERVER_PORT=7880
GRADIO_SERVER_NAME=0.0.0.0
